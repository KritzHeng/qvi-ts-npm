{
    "compilerOptions": {
      "target": "ES2020",
      "module": "ES2020",
      "lib": ["dom", "esnext"],
      "moduleResolution": "node",
      "strict": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "allowSyntheticDefaultImports": true,
      "esModuleInterop": true
    }
